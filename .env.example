# ミライアシスト環境設定ファイル例

# アプリケーション設定
VITE_APP_NAME=ミライアシスト
VITE_APP_VERSION=1.0.0

# セキュリティ設定
VITE_ENCRYPTION_KEY=your-encryption-key-here
VITE_SESSION_TIMEOUT=28800000
# 8時間（28800000ミリ秒）

# AI設定（ELYZA 7B instruct）
VITE_MODEL_PATH=./models/elyza-7b-instruct-q4_0.gguf
VITE_N_THREADS=
VITE_N_CTX=4096
VITE_MAX_TOKENS=800
VITE_TEMP=0.4
VITE_TOP_P=0.9
VITE_REPEAT_PENALTY=1.1
VITE_SEED=42

# ファイル設定
VITE_MAX_FILE_SIZE=10485760
# 10MB（10485760バイト）

# デバッグ設定
VITE_DEBUG_MODE=false
VITE_LOG_LEVEL=warn